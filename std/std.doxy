class Void;
class Str {
    fn str(self): Str {
        return self
    }
}

primitive Bool {
    fn str(self): Str {
        if self {
            return "true"
        }
        return "false"
    }
}

primitive Int {
    extern fn str(self): Str
}

extern fn print(msg: Str): Void;
extern fn print_nl(): Void;
extern fn exit(code: Int = 0): Void;
extern fn input(prompt: Str = "", buffer_size: Int = 1000): Str;