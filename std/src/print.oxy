def print (msg = "", line_end="\n") {
	let len = msg.len() * 8
	#asm Void "
	    mov rsi, qword [rbp + 16]
        mov rdx, qword [rbp - 8] ; string length
		mov rdi, 1
		call _Syscall.out
	"

	if !#unchecked_cast(Bool, line_end) {
		print("\n", "")
		return
	}

	if line_end != "" ->
		print(line_end, "")
}
