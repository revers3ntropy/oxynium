fn Char.== (self, other: Char) Bool {
	return #asm(Bool, "
		mov rax, qword [rbp + 16]
		cmp rax, qword [rbp + 24]
		sete al
		push rax
	")
}

fn Char.!= (self, other: Char) Bool {
	return !(self == other)
}

fn Char.str (self) Str {
	return #asm(Str, "
		push 16
		call Ptr.allocate
		add rsp, 8

		mov rdi, qword [rbp + 16]
		mov qword [rax], rdi
		push rax
	")
}

fn Char.is_digit (self) Bool {
	let as_int = self.as_int();
	return as_int >= 48 && as_int <= 57;
}

fn Char.as_int (self) Int {
	return Any.cast!<Char, Int>(self)
}

fn Char.from_int (i: Int) Char {
	return Any.cast!<Int, Char>(i)
}
