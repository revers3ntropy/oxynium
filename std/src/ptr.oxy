def Ptr.is_null (self) Bool ->
	Any.cast!<Ptr<T>, Int>(self) == 0

def Ptr.make <From> (val: From) Ptr<From> {
	return #asm Ptr<From> "
		mov rdi, 8
		mov rsi, 1
		call _LibC.calloc

		mov rdx, qword [rbp + 16]
		mov qword [rax], rdx
		push rax
	"
}

def Ptr.unwrap (self) T {
	return #asm T "
		mov rax, qword [rbp + 16]
		mov rax, qword [rax]
		push rax
	"
}

def Ptr.Str (self) Str ->
	"Ptr@" + Any.cast!<Ptr<T>, Int>(self).Str()
