def LibC.calloc (num: Int, size: Int) Ptr<Void> {
    return #asm Ptr<Void> "
        mov rdi, qword [rbp + 16]
        mov rsi, qword [rbp + 24]
        sub rsp, 32
        and rsp, -16
        call calloc
        push rax
    "
}

def LibC.free <T> (ptr: Ptr<T>) Void {
    #asm Void "
        mov rdi, qword [rbp + 16]
        sub rsp, 32
        and rsp, -16
        call free
    "
}
